{
  "version": 2,
  "builds": [
    {
      "src": "npm run backend",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "npm run backend"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
